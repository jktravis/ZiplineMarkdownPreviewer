{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "global": ["js", "jQuery"]
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [
      "error",
      "always"
    ]
  }
}